<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <!--android:top="1dp" android:left="1dp" android:right="1dp" android:bottom="1dp">-->

        <shape android:shape="rectangle">
            <solid android:color="#a49f98" />
            <corners
                android:topLeftRadius="30dp"
                android:topRightRadius="30dp"
                android:bottomLeftRadius="0dp"
                android:bottomRightRadius="30dp"/>
        </shape>

    </item>
    <item android:top="0.5dp" android:left="0.5dp" android:right="0.5dp" android:bottom="0.5dp">
        <shape android:shape="rectangle">
            <corners
                android:topLeftRadius="30dp"
                android:topRightRadius="30dp"
                android:bottomLeftRadius="0dp"
                android:bottomRightRadius="30dp"/>
            <solid android:color="@color/md_grey_50" />
        </shape>
    </item>
</layer-list>